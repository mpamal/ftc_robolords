    <MotorController name="Motor Controller drive" serialNumber="A104OQP6">
        <Motor name="left_drive" port="1" />
        <Motor name="right_drive" port="2" />
    </MotorController>
    <MotorController name="Motor Controller launch" serialNumber="A104OQ65">
        <Motor name="launch_motor2" port="1" />
        <Motor name="launch_motor1" port="2" />
    </MotorController>
