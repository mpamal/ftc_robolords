        <IrSeekerV3 name="irs" port="0" />
        <ColorSensor name="color_sensor" port="1" />
        <OpticalDistanceSensor name="ods" port="0" />
        <TouchSensor name="touch_sensor" port="0" />
    </DeviceInterfaceModule>
